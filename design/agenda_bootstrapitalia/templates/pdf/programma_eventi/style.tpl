{literal}
    <style>
        @page {
            margin: 0;
        }

        body {
            margin: 0;
            font-size: 10pt;
            font-family: "Titillium Web", "Arial", "Helvetica", sans-serif;
        }


        h1, h2, h3, h4 {
            font-size: 14pt;
            margin: 0 0 3pt;
            line-height: 1.2;
        }

        h1 {
            font-size: 18pt;
        }

        h2 {
            font-size: 16pt;
        }

        h3 {
            font-size: 13pt;
            font-weight: bold;
        }

        h4 {
            font-size: 11pt;
            font-weight: bold;
        }

        a {
            page-break-inside: avoid
        }

        p {
            margin: 0 0 1pt;
        }

        small{
            font-size: 8pt;
        }

        blockquote {
            page-break-inside: avoid;
        }

        h1, h2, h3, h4, h5, h6 {
            page-break-after: avoid;
            page-break-inside: avoid
        }

        img {
            page-break-inside: avoid;
            page-break-after: avoid;
        }

        table, pre {
            page-break-inside: avoid
        }

        ul, ol, dl {
            page-break-before: avoid
        }

        .calendar{
            height: 80px;
            width: 80px;
            border-radius: 4px;
            background-color: #fff;
            text-align: center;
            font-weight: 600;
            line-height: 1.3;
            text-transform: uppercase;
            font-size: 10pt;
            border: 1px solid;
        }

        .calendar .date{
            font-size: 23pt;
            line-height: 1.5;
        }

        .calendar .date small{
            color:#bbb;
            font-size: 15pt;
            text-transform: lowercase;
        }

        .m-top{
            margin-top: 20px;
        }

        .p-all{
            padding: 20px;
        }

        .text-center{
            text-align: center;
        }

    </style>
{/literal}
